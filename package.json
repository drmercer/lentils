{
  "name": "lentils",
  "version": "0.0.1",
  "description": "A hodge-podge of useful TypeScript",
  "scripts": {
    "test:unit": "jest",
    "test:integration": "test/test.sh",
    "ts-check": "tsc --project tsconfig.jest.json --noEmit"
  },
  "gitHooks": {
    "pre-commit": "yarn test:unit && yarn test:integration"
  },
  "author": "Dan Mercer",
  "private": true,
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.5",
    "yorkie": "^2.0.0"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "tslib": "^2.0.3"
  }
}
